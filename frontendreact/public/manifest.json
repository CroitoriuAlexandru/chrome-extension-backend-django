{
  "manifest_version": 3,
  "name": "Organigram And More",
  "version": "1.0",
  "description": "Side Panel Extension",
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "Click to open extension (hover message)"
  },
  "permissions": ["sidePanel"],
  "side_panel": {
    "default_path": "index.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+Q"
      }
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
